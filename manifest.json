{
	"manifest_version": 3,
	"name": "Session Manager",
	"version": "3.5.1",
	"description": "Simple yet powerful tab set management. Quickly and easily save, update, remove, and restore sets of tabs! Tha main focus of this update is to make it compatible with Chrome. This is a fork of the project: https://github.com/tddyco/session-manager",
	"icons": {
		"16": "img/16.png",
		"128": "img/128.png"
	},
	"action": {
		"default_icon": {
			"19": "img/19-1x.png",
			"38": "img/19-2x.png"
		},
		"default_title": "Session Manager",
		"default_popup": "popup.html"
	},
	"background": {
		"service_worker": "js/main.js",
		"type": "module"

	},
	"content_security_policy": {
		"extension_pages": "script-src 'self'; object-src 'self'"
	},
	"offline_enabled": true,
	"omnibox": {
		"keyword": "session"
	},
	"options_page": "options.html",
	"options_ui": {
		"page": "options.html",
		"open_in_tab": true
	},
	"permissions": [
		"tabs",
		"storage"
	]
}
